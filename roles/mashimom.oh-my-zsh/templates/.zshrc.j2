# File generated by Ansible, CHANGES HERE MAY NOT PERSIST!!!

export ZSH=$HOME/.oh-my-zsh

## Theme
ZSH_THEME="{{ ohmyzsh_theme }}"

## plugins
plugins=({{ ohmyzsh_plugins|join(' ') }})

export PATH=$HOME/bin:/usr/local/bin:$PATH

source $ZSH/oh-my-zsh.sh

## aliases
alias prn="print -l $@"
alias fnd="realpath $@"
alias findjar="for f (**/*.jar(.)) jar -tf \"$f\" | grep --label \"$f\" -l `sed 's|\.|/|g' <<< \"$@\"`"


## cedric custom alias
alias c="clear"
alias dc start="sudo service docker start"
alias dc stop="sudo service docker stop"
alias dc v="docker version"
alias dc restart="sudo service docker restart"


